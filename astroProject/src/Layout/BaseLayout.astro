---
const {title} = Astro.props
import Navbar from '../components/Navbar.astro'
import 'aos/dist/aos.css'
import Footer from '../components/Footer.astro'
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <link rel="icon" type="image/x-icon" href="/favicon.ico" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>

    <meta name="description" content="Explore the latest in premium footwear with Jumman Traders. Shop high-quality shoes for men. Find your perfect pair today!">
    <meta name="keywords" content="Jumman Traders, shoes, footwear, men shoes, master shoes, premium footwear, online shoe store">
    <meta name="author" content="Jumman Traders">


<!-- Open Graph / Facebook -->
<meta property="og:type" content="website">
<meta property="og:url" content="https://jummantraders.com/">
<meta property="og:title" content="Jumman Traders - Premium Footwear Collection">
<meta property="og:description" content="Discover premium shoes for all styles at Jumman Traders. Perfect footwear for every occasion. Shop now!">
<meta property="og:image" content="https://jummantraders.com/android-chrome-512x512.png"> <!-- Replace with your OG image URL -->

<!-- Twitter -->
<meta property="twitter:card" content="summary_large_image">
<meta property="twitter:url" content="https://jummantraders.com/">
<meta property="twitter:title" content="Jumman Traders - Premium Footwear Collection">
<meta property="twitter:description" content="Find the best shoes at Jumman Traders. High-quality footwear for men. Shop now!">
<meta property="twitter:image" content="https://jummantraders.com/favicon-32x32.png"> <!-- Replace with your Twitter image URL -->

<!-- Favicon -->
<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
<link rel="apple-touch-icon" href="/apple-touch-icon.png">
<link rel="manifest" href="/site.webmanifest"> <!-- Optional for PWA -->
<!-- Robots -->
<meta name="robots" content="index, follow">

<!-- Canonical URL -->
<link rel="canonical" href="https://jummantraders.com/">

<!-- Mobile Web App Capable -->
<meta name="mobile-web-app-capable" content="yes">
<meta name="theme-color" content="#ffd60a">


    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Kanit:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">

  <!-- Bootstrap CSS -->
  <link
  href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
  rel="stylesheet"
  integrity="sha384-..."
  crossorigin="anonymous"
/>
    <!-- Swiper CSS -->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.css"
    />
        <!-- AOS CSS -->
        <link
        rel="stylesheet"
        href="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.css"
      />
    <link rel="stylesheet" href="/CSS/Global.css" />


  </head>
  <body>
    <Navbar/>
    <main>
        <slot />
     
        <!--
    
        <script>
          import AOS from "aos";
          AOS.init({
            duration:1000,
      easing:"ease-in-out",
      once:false,
      mirror:false
          })
        </script>
        -->

    </main>
    <Footer/>
    <!-- Swiper JS -->
    <script src="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-..."
      crossorigin="anonymous"
    ></script>
    <!--
            <script src="/node_modules/bootstrap/dist/js/bootstrap.bundle.min.js" is:inline></script>

    -->
            <!-- AOS JS -->
    <script src="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.js"></script>

        
     <!-- Initialize AOS -->
     <script>
      // @ts-ignore
      AOS.init({
            duration:1000,
      easing:"ease-in-out",
      once:false,
      mirror:false
          });
    </script>
  </body>
</html>
